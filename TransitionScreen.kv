<TransitionScreen>

   name: 'TransitionScreen'

   FloatLayout:
      size_hint: None, None

   ImageButton:
      id: imageBtn
      source: "../../pidev/kivy/images/DPEALogoTransparent.png"
      size: 100, 100
      x: root.width * 0.5
      y: root.height * 0.5
      on_press: root.transitionback()